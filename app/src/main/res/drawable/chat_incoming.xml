<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!--Shadow Layer-->


    <item android:right="@dimen/dp10">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp25" />
            <padding
                android:bottom="1px"
                android:left="1px"
                android:right="1px" />
            <solid android:color="#01000000" />
        </shape>
    </item>

    <!--===============-->


    <item android:right="@dimen/dp10">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp25" />
            <padding android:bottom="1px" />
            <solid android:color="#09000000" />
        </shape>
    </item>

    <!--===============-->


    <item android:right="@dimen/dp10">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp25" />
            <padding
                android:bottom="1px"
                android:left="1px"
                android:right="1px" />
            <solid android:color="#10000000" />
        </shape>
    </item>

    <!--===============-->


    <!--ForeGround-->


    <item android:right="@dimen/dp10">
        <shape android:shape="rectangle">
            <solid android:color="@color/tabL_grey_lGray" />
            <corners android:bottomLeftRadius="0dp" android:radius="@dimen/dp25" />
        </shape>
    </item>

</layer-list>